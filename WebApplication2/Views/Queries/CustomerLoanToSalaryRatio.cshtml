@model IEnumerable<CustomerLoanToSalaryRatioViewModel>

<h2>Customer Loan to Salary Ratio</h2>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Loan To Salary Ratio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.LoanToSalaryRatio</td>
            </tr>
        }
    </tbody>
</table>
